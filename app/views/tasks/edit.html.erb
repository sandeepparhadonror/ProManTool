<div class="row">
  <div class="col-sm-4">
    <h2>Edit Project</h2>  	
  </div>
  <div class="col-sm-2">
    <%= link_to "Back To Projects", projects_path, class: "btn btn-info" %>
  </div>
</div>
	
<br><br>

<%= form_for [@project, @task] do |task| %>
  <form>
	
		<%= render "form", task: task  %>

		<br>
		<div class="form-group row">
			<div class="col-sm-3">
				<button type="submit" class="btn btn-primary">Update Task</button>
			</div>
		</div>
	</form>


<% end %>